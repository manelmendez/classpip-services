{
  "name": "Card",
  "plural": "cards",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "string",
      "required": true
    },
    "ratio": {
      "type": "string",
      "required": true
    },
    "rank": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "collectionCards": {
      "type": "belongsTo",
      "model": "CollectionCard",
      "foreignKey": "collectionCardId"
    },
    "student": {
      "type": "hasAndBelongsToMany",
      "model": "Student",
      "foreignKey": "studentId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}

